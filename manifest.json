{
    "manifest_version": 3,
    "name": "Simple ServiceNow Time Entry",
    "version": "1.0",
    "description": "Fills 15 minutes time worked with a single button.",
    "permissions": [
      "activeTab",
      "scripting",
      "webNavigation"
    ],
    "host_permissions": [
      "*://*.service-now.com/*"
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }